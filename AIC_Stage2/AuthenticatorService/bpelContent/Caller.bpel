          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:CallerResponseMessage"/>
        <bpel:variable name="DSLinkResponse" messageType="ns:IAuthenticator_validateLogin_OutputMessage"></bpel:variable>
        <bpel:variable name="DSLinkRequest" messageType="ns:IAuthenticator_validateLogin_InputMessage"></bpel:variable>
        
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in Caller.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:Caller"
                 operation="process" variable="input"
                 createInstance="yes"/>
        
        <!-- Generate reply to synchronous request -->
        <bpel:assign validate="no" name="Assign">
            
            
            <bpel:copy>
                <bpel:from><bpel:literal><tns:getStatisticValue xmlns:tns="http://tempuri.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:username>tns:username</tns:username>
</tns:getStatisticValue>
</bpel:literal></bpel:from>
                <bpel:to variable="DSLinkRequest" part="parameters"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="payload" variable="input">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:input]]></bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="DSLinkRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns:username]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="Invoke" partnerLink="DSLink" operation="getStatisticValue" inputVariable="DSLinkRequest" outputVariable="DSLinkResponse"></bpel:invoke>
        
        <bpel:assign validate="no" name="Assign1">
            
            <bpel:copy>
                <bpel:from><bpel:literal><tns:CallerResponse xmlns:tns="http://MyTest.com/Test" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:result>tns:result</tns:result>
</tns:CallerResponse>
</bpel:literal></bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="DSLinkResponse">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[ns:getStatisticValueResult]]>
                    </bpel:query>
                </bpel:from>
                <bpel:to part="payload" variable="output">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[tns:result]]>
                    </bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:Caller"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
